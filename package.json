{
  "name": "tau",
  "productName": "tau",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".vite/build/main.js",
  "private": true,
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "test": "vitest run",
    "test:watch": "vitest",
    "daemon": "tsx src/daemon/index.ts",
    "daemon:build": "esbuild src/daemon/index.ts --bundle --platform=node --target=node20 --outdir=dist/daemon --external:@mariozechner/pi-coding-agent --external:ws",
    "make:dmg": "electron-forge make --targets @electron-forge/maker-dmg"
  },
  "keywords": [],
  "author": {
    "name": "wundervaflja",
    "email": "gorysko@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.11.1",
    "@electron-forge/maker-deb": "^7.11.1",
    "@electron-forge/maker-dmg": "^7.11.1",
    "@electron-forge/maker-rpm": "^7.11.1",
    "@electron-forge/maker-squirrel": "^7.11.1",
    "@electron-forge/maker-zip": "^7.11.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.11.1",
    "@electron-forge/plugin-fuses": "^7.11.1",
    "@electron-forge/plugin-vite": "^7.11.1",
    "@electron/fuses": "^1.8.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@types/ws": "^8.5.12",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^5.1.3",
    "autoprefixer": "^10.4.24",
    "electron": "40.2.1",
    "esbuild": "^0.24.2",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.32.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.19.2",
    "typescript": "~4.5.4",
    "vite": "^5.4.21",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@mariozechner/pi-coding-agent": "^0.52.6",
    "@xterm/addon-fit": "^0.11.0",
    "@xterm/addon-web-links": "^0.12.0",
    "@xterm/xterm": "^6.0.0",
    "electron-squirrel-startup": "^1.0.1",
    "mermaid": "^11.12.2",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "ws": "^8.18.0"
  }
